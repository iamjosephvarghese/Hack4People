<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hackathon.rm.com.hack4people">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Ads">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".Splash" />
        <activity android:name=".MiddleList" />
        <activity android:name=".SoundRecording" />
        <activity android:name=".UploadAds" />
        <activity android:name=".AdsDetailed" />
        <activity android:name=".ViewAudio" />
        <activity android:name=".ShowSelected" />
        <activity android:name=".FarmerUpload" />
        <activity android:name=".MiddleDetailed"/>
    </application>

</manifest>